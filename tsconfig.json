{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strictNullChecks": false,
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "@assets": ["src/assets"],
      "@assets*": ["src/assets*"],
      "@hooks": ["src/hooks"],
      "@hooks*": ["src/hooks*"],
      "@styles": ["src/styles"],
      "@styles*": ["src/styles*"],
      "@providers": ["src/components/providers"],
      "@providers*": ["src/components/providers*"],
      "@atoms": ["src/components/atoms"],
      "@atoms*": ["src/components/atoms*"],
    },
  },
}
